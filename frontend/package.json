{
  "name": "autopromote-frontend",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@sentry/react": "^7.120.4",
    "@sentry/tracing": "^7.72.0",
    "autopromote-server": "file:..",
    "firebase": "^12.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.6.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "format": "prettier --write \"src/**/*.{js,jsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css}\"",
    "lint": "eslint --config .eslintrc.json --env es2021 --ext .js,.jsx src --max-warnings=0",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "git add docs && git commit -m \"Update docs for GitHub Pages\" && git push"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "cpy": "^12.0.1",
    "eslint": "^8.45.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "gh-pages": "^6.3.0"
  },
  "overrides": {
    "nth-check": "2.1.1",
    "postcss": "8.5.6",
    "glob": "^11.1.0",
    "js-yaml": "^4.1.1",
    "node-forge": "1.3.2",
    "serialize-javascript": "6.0.2"
  }
}
