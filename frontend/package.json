{
  "homepage": "https://tibule12.github.io/AutoPromote/",
  "name": "autopromote-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "firebase": "^12.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean-docs": "rimraf docs",
    "build": "npm run clean-docs && react-scripts build && cpy build/**/* docs --parents",
    "build:win": "npm run clean-docs && react-scripts build && xcopy build docs /e /i /h /y",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d docs"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "rimraf": "^3.0.2",
    "cpy-cli": "^4.2.0"
  }
}
